{
  "name": "metropolis-ui-boilerplate",
  "version": "0.0.1",
  "description": "Metropolis boilerplate used to create new UI projects",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "precommit-msg": "echo 'Running pre-commit checks...' && exit 0",
    "remove-dist": "rimraf ./dist",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* client/src tools --color",
    "lint:styles": "stylelint client/assets/styles/**/*.scss --color",
    "lint:watch": "npm run lint -- --watch",
    "lint-staged": "lint-staged",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test:jenkins",
    "build": "babel-node tools/build.js",
    "test": "NODE_PATH=./client mocha tools/testSetup.js \"client/spec/**/*.spec.js\" --reporter spec",
    "test:debug": "karma start karma.config.js",
    "test:jenkins": "JUNIT_REPORT_PATH=./report.xml npm run test -- --reporter mocha-jenkins-reporter --colors ",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"./{,!(node_modules)/**/}*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch",
    "scm": "scm-source > scm-source.json",
    "version": "node -e \"console.log(require('./package.json').version)\"",
    "open:cover": "npm run test:cover && open coverage/index.html",
    "stats": "webpack-bundle-analyzer dist/stats.json dist"
  },
  "author": "",
  "private": true,
  "lint-staged": {
    "*.js": "eslint --color",
    "*.scss": "stylelint --color"
  },
  "precommit": {
    "run": [
      "precommit-msg",
      "lint-staged"
    ]
  },
  "dependencies": {
    "antd": "2.5.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "express": "~4.14.0",
    "express-http-proxy": "~0.10.0",
    "firebase": "^3.9.0",
    "normalizr": "2.3.0",
    "prop-types": "^15.5.9",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-import": "1.1.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.16.3",
    "browser-sync": "2.17.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "connect-pause": "0.1.0",
    "coveralls": "2.11.14",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "ejs": "^1.0.0",
    "enzyme": "2.5.1",
    "eslint": "3.8.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jsdom": "9.9.1",
    "jsdom-global": "2.1.1",
    "json-loader": "0.5.4",
    "json-server": "0.9.4",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-mocha": "1.3.0",
    "karma-sinon": "1.0.5",
    "karma-sinon-chai": "1.2.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.1",
    "lint-staged": "3.2.6",
    "mocha": "3.1.2",
    "mocha-jenkins-reporter": "0.3.5",
    "mockdate": "1.0.4",
    "node-sass": "3.10.1",
    "node-scm-source": "0.2.0",
    "npm-run-all": "3.1.1",
    "open": "0.0.5",
    "postcss-loader": "1.0.0",
    "pre-commit": "1.2.2",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.3.2",
    "redux-immutable-state-invariant": "1.2.4",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "stylelint": "7.7.1",
    "stylelint-config-sass-guidelines": "1.1.1",
    "stylelint-scss": "1.4.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.0",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
